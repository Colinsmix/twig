<div class="contain-to-grid">
  <div class='small-9 small-offset-1 small-centered columns'>
    <nav class="top-bar" data-topbar>
      <ul class="title-area">
        <li class="name">
          <h1><%= link_to 'LS-Twig', root_path %></h1>
        </li>
      </ul>
      <section class="top-bar-section">
        <ul class="right">
          <% if user_signed_in? %>
            <li class="name"><%= link_to "Sign Out", destroy_user_session_path, method: :delete %></li>
          <% else %>
            <li class="name"><%= link_to "Sign In", user_omniauth_authorize_path(:google_oauth2) %><br></li>
          <% end %>
        </ul>
      </section>
    </nav>
  </div>
  <div class="small-1 columns">
  </div>
</div>
